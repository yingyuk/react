{
  "presets": [
    ["env", {
      "modules": false
    }],
    "stage-2",
    "react"
  ],
  "plugins": [
    "transform-runtime", ["component", [{
      "libraryName": "mint-ui",
      "style": true
    }]]
  ],
  "comments": false,
  "env": {
    "test": {
      "presets": ["env", "stage-2"],
      "plugins": ["istanbul"]
    }
  }
}